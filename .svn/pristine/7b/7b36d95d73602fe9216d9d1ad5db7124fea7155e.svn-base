import { prepare } from "@/services/api";
import { request, METHOD } from "@/utils/request";

//报名
//获取查询列
export async function getSignUpList(data) {
  return request(
    `${prepare}/esBSubTender/findSubTenderForPage`,
    METHOD.GET,
    data
  );
}
//提交报名
export async function submitSignUp(data) {
  return request(
    `${prepare}/esBSubTender/finishSubTender`,
    METHOD.POST,
    data
  );
}
